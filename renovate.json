{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prHeader": "ADCM-Bot-Header",
  "assignees": [
    "mrdaviscsit"
  ],
  "labels": ["dependencies-updater"],
  "packageRules": [
    {
      "matchPackagePatterns": ["mkdocs"],
      "labels": ["mkdocs-updates"]
    },
    {
      "matchPackagePatterns": ["mkdocs-autorefs"],
      "labels": ["mkdocs-autorefs-updates"],
    {
      "matchPackagePatterns": ["mkdocstrings-python"],
      "labels": ["mkdocstrings-python-updates"]
    },
    {
      "matchPackagePatterns": ["mkdocstrings"],
      "labels": ["mkdocstrings-updates"]
    }
  ]
}
